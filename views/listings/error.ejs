<% layout("layouts/boilerplate") %>

<link rel="stylesheet" href="/css/error.css">

<body>

    <br>
    <div class="alert alert-danger" role="alert">
        <h4>Error <%=  err.statusCode %></h4>
        <p><%=  err.message %></p>
        <% if(err.stack){ %>
            <pre><%=  err.stack %></pre>
        <% } %>
    </div>

</body>
